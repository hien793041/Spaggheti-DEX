{
    "version": 2,
    "build": [
        {
            "src": "/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/artifacts/(.*)",
            "dest": "/artifacts/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}